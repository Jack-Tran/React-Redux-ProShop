l>Email</Form.Label>\r\n          <Form.Control\r\n            type=\"email\"\r\n            placeholder=\"Enter email\"\r\n            value={email}\r\n            onChange={(event) => setEmail(event.target.value)}\r\n          ></Form.Control>\r\n        </Form.Group>\r\n\r\n        <Form.Group controlId=\"password\">\r\n          <Form.Label>Password</Form.Label>\r\n          <Form.Control\r\n            type=\"password\"\r\n            placeholder=\"Enter password\"\r\n            value={password}\r\n            onChange={(event) => setPassword(event.target.value)}\r\n          ></Form.Control>\r\n        </Form.Group>\r\n\r\n        <Form.Group controlId=\"confirmPassword\">\r\n          <Form.Label>Confirm Password</Form.Label>\r\n          <Form.Control\r\n            type=\"password\"\r\n            placeholder=\"Confirm password\"\r\n            value={confirmPassword}\r\n            onChange={(event) => setConfirmPassword(event.target.value)}\r\n          ></Form.Control>\r\n        </Form.Group>\r\n\r\n        <Button type=\"submit\" variant=\"primary\">\r\n          Register\r\n        </Button>\r\n      </Form>\r\n      <Row className=\"py-3\">\r\n        <Col>\r\n          Already have an account?{\" \"}\r\n          <Link to={redirect ? `/login?redirect=${redirect}` : \"/login\"}>\r\n            Login\r\n          </Link>\r\n        </Col>\r\n      </Row>\r\n    </FormContainer>\r\n  );\r\n};\r\n\r\nexport default RegisterPage;\r\n","C:\\Users\\Jack\\Desktop\\React\\E-Commerce\\sandbox\\client\\src\\pages\\ProfilePage.js",[],"C:\\Users\\Jack\\Desktop\\React\\E-Commerce\\sandbox\\client\\src\\components\\Rating.js",[],"C:\\Users\\Jack\\Desktop\\React\\E-Commerce\\sandbox\\client\\src\\pages\\ShippingPage.js",[],"C:\\Users\\Jack\\Desktop\\React\\E-Commerce\\sandbox\\client\\src\\components\\CheckoutProcess.js",[],"C:\\Users\\Jack\\Desktop\\React\\E-Commerce\\sandbox\\client\\src\\pages\\PaymentPage.js",[],"C:\\Users\\Jack\\Desktop\\React\\E-Commerce\\sa